{
  "name": "@primero.ai/agents-helpers",
  "version": "1.1.2",
  "description": "primero.ai - Agents helpers.",
  "license": "MIT",
  "author": "primero.ai",
  "type": "module",
  "scripts": {
    "with-env": "dotenv -e .env --",
    "build": "tsc --project tsconfig.json",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm run build",
    "clean": "rm -rf node_modules dist"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./resource-query/client": {
      "types": "./dist/resource-query/client.d.ts",
      "import": "./dist/resource-query/client.js"
    },
    "./resource-query/schemas": {
      "types": "./dist/resource-query/schemas.d.ts",
      "import": "./dist/resource-query/schemas.js"
    },
    "./resource-query/types": {
      "types": "./dist/resource-query/types.d.ts",
      "import": "./dist/resource-query/types.js"
    }
  },
  "dependencies": {
    "axios": "^1.13.5",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.2.3",
    "dotenv": "^17.3.1",
    "dotenv-cli": "^11.0.0",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.29.3"
}
